<p id="notice"><%= notice %></p>

<h1>Part2s</h1>

<table>
  <thead>
    <tr>
      <th>Ans1</th>
      <th>Ans2</th>
      <th>Ans3</th>
      <th>Ans4</th>
      <th>Ans5</th>
      <th>Ans6</th>
      <th>Ans7</th>
      <th>Ans8</th>
      <th>Ans9</th>
      <th>Ans10</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @part2s.each do |part2| %>
      <tr>
        <td><%= part2.ans1 %></td>
        <td><%= part2.ans2 %></td>
        <td><%= part2.ans3 %></td>
        <td><%= part2.ans4 %></td>
        <td><%= part2.ans5 %></td>
        <td><%= part2.ans6 %></td>
        <td><%= part2.ans7 %></td>
        <td><%= part2.ans8 %></td>
        <td><%= part2.ans9 %></td>
        <td><%= part2.ans10 %></td>
        <td><%= link_to 'Show', part2 %></td>
        <td><%= link_to 'Edit', edit_part2_path(part2) %></td>
        <td><%= link_to 'Destroy', part2, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Part2', new_part2_path %>
