<p id="notice"><%= notice %></p>

<h1>Part4s</h1>

<table>
  <thead>
    <tr>
      <th>Ans1</th>
      <th>Ans2</th>
      <th>Ans3</th>
      <th>Ans4</th>
      <th>Ans5</th>
      <th>Ans6</th>
      <th>Ans7</th>
      <th>Ans8</th>
      <th>Ans9</th>
      <th>Ans10</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @part4s.each do |part4| %>
      <tr>
        <td><%= part4.ans1 %></td>
        <td><%= part4.ans2 %></td>
        <td><%= part4.ans3 %></td>
        <td><%= part4.ans4 %></td>
        <td><%= part4.ans5 %></td>
        <td><%= part4.ans6 %></td>
        <td><%= part4.ans7 %></td>
        <td><%= part4.ans8 %></td>
        <td><%= part4.ans9 %></td>
        <td><%= part4.ans10 %></td>
        <td><%= link_to 'Show', part4 %></td>
        <td><%= link_to 'Edit', edit_part4_path(part4) %></td>
        <td><%= link_to 'Destroy', part4, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Part4', new_part4_path %>
